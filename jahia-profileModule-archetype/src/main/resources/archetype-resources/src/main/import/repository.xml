<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:jnt="http://www.jahia.org/jahia/nt/1.0"
              xmlns:jcr="http://www.jcp.org/jcr/1.0"
              xmlns:jmix="http://www.jahia.org/jahia/mix/1.0" xmlns:j="http://www.jahia.org/jahia/1.0"
              xmlns:mix="http://www.jcp.org/jcr/mix/1.0" >
    <templateSets jcr:primaryType="jnt:templateSets">
        <${artifactId} jcr:primaryType="jnt:virtualsite" j:installedModules="${artifactId}"
            j:siteType="profileModule" j:dependencies="templates-system">
            <portlets jcr:primaryType="jnt:portletFolder"/>
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder">
                <profile-page-items jcr:primaryType="jnt:contentFolder">
                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:primaryType="jnt:translation"
                                      jcr:title="Profile Page Items"/>
                    <${artifactId} divClass="user-${artifactId}" jcr:primaryType="jnt:componentLink"
                                         targetTemplate="user-${artifactId}">
                        <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" jcr:title="${artifactId}"/>
                    </${artifactId}>
                </profile-page-items>
            </contents>
            <templates jcr:primaryType="jnt:templatesFolder" j:rootTemplatePath="/base/user-base">
                <files jcr:primaryType="jnt:folder">
                    <themes jcr:primaryType="jnt:folder">
                        <default jcr:primaryType="jnt:folder">

                        </default>
                    </themes>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <user-${artifactId} j:applyOn="jnt:user" j:defaultTemplate="false" j:hiddenTemplate="false"
                                          jcr:primaryType="jnt:contentTemplate" j:priority="10">
                    <pagecontent jcr:primaryType="jnt:contentList">
                    </pagecontent>
                </user-${artifactId}>
            </templates>
        </${artifactId}>
    </templateSets>
</content>
        